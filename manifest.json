{
		"manifest_version": 2,
		"name": "Navimacs",
		"version": "1.1",
		"description": "WebExtension for Emacs web-page navigation",
		"icons": {
				"48": "icons/emacs.png",
				"96": "icons/emacs.png"
		},
		"author": "Denis Rechkunov",
		"homepage_url": "https://github.com/pragmader/navimacs",
		"content_scripts": [
				{
						"js": [
								"navimacs.js"
						],
						"matches": [
								"<all_urls>"
						],
						"match_about_blank": true,
						"all_frames": true
				}
		]
}
